generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id          Int         @id @default(autoincrement())
  firstName   String
  lastName    String
  email       String      @unique
  credentials Credential?
}

model Credential {
  userId   Int    @id
  user     User   @relation(fields: [userId], references: [id])
  password String
}
